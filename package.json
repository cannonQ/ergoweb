    "@next/mdx": "^12.3.4",
    "@types/gtag.js": "^0.0.10",
    "@types/lodash": "^4.14.202",
    "body-scroll-lock": "^4.0.0-beta.0",
    "date-fns": "^3.1.0",
    "feed": "^4.2.2",
    "framer-motion": "^6.5.1",
    "lodash": "^4.17.21",
    "next": "^13.5.6",
    "next-sitemap": "^3.1.55",
    "next-themes": "^0.0.15",
    "nextjs-progressbar": "0.0.14",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-countup": "^6.5.0",
