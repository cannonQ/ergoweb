    "react-icons": "^4.12.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^8.34.0",
    "react-intl": "^5.25.1",
    "react-markdown": "^8.0.7",
    "react-toastify": "^8.2.0",
    "rehype-raw": "^6.1.1",
    "remark-breaks": "^3.0.3",
    "remark-gfm": "^3.0.1",
    "remove-markdown": "^0.3.0",
    "sass": "^1.69.5",
    "showdown": "^2.1.0",
    "swiper": "^8.4.7",
    "tw-elements": "^1.1.0"
