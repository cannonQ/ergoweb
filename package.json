    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "classnames": "^2.5.1",
    "eslint": "8.7.0",
    "eslint-config-next": "^12.3.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "tailwindcss-gradient": "^1.0.1",
    "typescript": "4.5.4"
